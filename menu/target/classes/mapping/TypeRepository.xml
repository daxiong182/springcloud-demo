<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.courage.repostories.TypeRepository">


    <select id="findById" resultType="com.courage.entity.Type">
        select * from t_type where id=#{id}
    </select>

    <select id="findAll" resultType="com.courage.entity.Type">
        select * from t_type
    </select>

</mapper>